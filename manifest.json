{
  "manifest_version": 3,
  "name": "Momentum Lite",
  "version": "1.0",
  "description": "A minimal Momentum-like Chrome extension with essential features.",
  "permissions": ["tabs"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
}
